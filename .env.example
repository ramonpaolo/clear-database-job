# Query
FIELD_DATE='created_at'
OPTIONAL_QUERIES='{"event": "trialing"}'

# Notification
NOTIFICATION_PROVIDER="sendgrid"

# Sendgrid
SENDGRID_API_KEY="SG."
TO_EMAIL="TARGET_EMAIL"
FROM_EMAIL="ORIGIN_EMAIL"
