# App
APP_NAME='cron-job'
NODE_ENV='development'

# CronJob
EXECUTE_WHEN_INIT='true'
EXECUTE_EVERY_TIME=1
EXECUTE_TIME_UNIT='minutes'

# MongoDB
MONGO_PASSWORD='test'
MONGO_USERNAME='test'

MONGO_URL="mongodb://${MONGO_USERNAME}:${MONGO_PASSWORD}@mongo"

